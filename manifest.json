{
  "description": "Open all Firefox windows in maximized mode",
  "manifest_version": 2,
  "name": "AutoMaximize",
  "version": "2.0.4",
  "homepage_url": "https://github.com/Zirchona/AutoMaximize",
  "background": {
    "scripts": [
      "startup.js"
    ]
  },
  "icons": {
    "64": "icons/fullscreen_64x64.png",
    "128": "icons/fullscreen_128x128.png"
  },
  "applications": {
    "gecko": {
      "strict_min_version": "48.0"
    }
  }
}
